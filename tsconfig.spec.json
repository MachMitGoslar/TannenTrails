/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine"
    ],
      "paths": {
    "@app/*": ["src/app/*"],
    "@assets/*": ["src/assets/*"],
    "@env/*": ["src/environments/*"],
    "@models/*": ["src/app/models/*"]
  },
  },

  "files": [
    "src/test.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts"
  ]
}
